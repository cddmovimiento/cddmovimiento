<odoo>
  <data>
    <!-- account.move inherit form view -->
    <record id="view_partner_form_inh_cdd_sale_fleet" model="ir.ui.view">
        <field name="name">res.partner.inherit.cdd.sale</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='phone']" position="after">
                <field name="fleet_services_config_id" invisible="1"/>
            </xpath>

        </field>
    </record>
      
  </data>
</odoo>


